<%= simple_form_for(@minute, url: item_minute_path(@item, @minute)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :text %>
    <%= f.association :minst, label: "Status", include_blank: false %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-warning" %>
  </div>
<% end %>
